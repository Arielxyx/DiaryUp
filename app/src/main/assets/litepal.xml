<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--数据库名-->
    <dbname value="DiaryUp"></dbname>
    <!--数据库版本号,如果修改model类了，可以修改版本号，这样，之前数据库中的数据也能得到保存-->
    <version value="1"></version>
    <!--映射模型，注意一定要使用完整的类名-->
    <list>
        <mapping class="com.example.diaryup.bean.Diary"></mapping>
    </list>
</litepal>